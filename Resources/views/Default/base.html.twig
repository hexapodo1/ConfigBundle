<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}App Config System{% endblock %}</title>

    <!-- Bootstrap -->
    <link href="{{ asset('bundles/config/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/config/jquery-ui-1.11.4/jquery-ui.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/config/jstree/themes/default/style.min.css') }}" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
    <body>
        <div class="container">
            {% block content %}
                <h1>Page content</h1>
            {% endblock%}
        </div>

      <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
      <script src="{{ asset('bundles/config/jquery/jquery-2.2.4.min.js') }}"></script>
      <script src="{{ asset('bundles/config/bootstrap/js/bootstrap.min.js') }}"></script>
      <script src="{{ asset('bundles/config/jquery-ui-1.11.4/jquery-ui.min.js') }}"></script>
      <script src="{{ asset('bundles/config/jstree/jstree.min.js') }}"></script>
      <!-- Include all compiled plugins (below), or include individual files as needed -->
      {% block js %}
      {% endblock %}
    </body>
</html>